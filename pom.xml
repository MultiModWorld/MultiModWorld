      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <organization>
  	<name>MultiModWorld</name>
  	<url>https://github.com/MultiModWorld</url>
  </organization>
  <scm>
  	<url>https://github.com/MultiModWorld/MultiModWorld</url>
  </scm>
  <issueManagement>
  	<system>GitHub</system>
  	<url>https://github.com/MultiModWorld/MultiModWorld/issues</url>
